{
  "name": "dylanvann-com",
  "version": "1.0.0",
  "description": "Dylan Vann's website.",
  "keywords": [
    "gatsby"
  ],
  "homepage": "https://dylanvann.com",
  "bugs": {
    "url": "https://github.com/dylanvann/dylanvann-com/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylanvann/dylanvann-com.git"
  },
  "license": "MIT",
  "author": "Dylan Vann <dylan@dylanvann.com>",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "check-spelling": "cspell 'posts/**/*.mdx'",
    "clean": "gatsby clean",
    "format:check": "prettier --check 'src/**/*' --ignore-unknown",
    "format:fix": "prettier --write 'src/**/*' --ignore-unknown",
    "postinstall": "patch-package",
    "lint:check": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,tsx}'",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test-ci": "run-s check-spelling format:check lint:check"
  },
  "prettier": {
    "svelteBracketNewLine": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "resolutions": {
    "puppeteer": "5.2.1"
  },
